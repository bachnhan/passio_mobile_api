<script>
    var listAccounts = [];
    $(document).ready(function () {
        initDataTable(listAccounts);
        $('#btn-back').fixedScroll(70);
        $('#btn-submit').fixedScroll(70);
        $('#Date_Create').val(moment().format("DD/MM/YYYY"));
        LoadCardType();
        var now = new Date();
        GetProductList();
        $('.date-picker-date').datepicker({
            //changeYear: true,
            showButtonPanel: true,
            format: 'dd/mm/yyyy',
            //viewMode: "years",
            orientation: "left",
            //endDate: new Date(),
            autoclose: true,
            //minViewMode: "years",
            onClose: function (dateText, inst) {
                var year = $("#ui-datepicker-div .ui-datepicker-year :selected").val();
                $(this).datepicker('setDate', new Date(year, 1));
            }
        });
        //$(".date-picker-date").focus(function () {
        //    $(".ui-datepicker-month").hide();
        //});
    });
    $('#checkboxSample').on('click', function () {
        if ($('#checkboxSample').prop('checked') == true) {
            $('#sampleDiv').show();
            $('#btnCreateDiv').removeClass("p-t-20");
        }
        else {
            $('#sampleDiv').hide();
            $('#btnCreateDiv').addClass("p-t-20");
        }
    });
    function LoadCardType() {
        $.ajax({
            type: 'POST',
            url: '@Url.Action("LoadAllMembershipType","MembershipCard")',
            success: function (result) {
                var content = "";
                for (var i = 0; i < result.length; i++) {
                    content += '<option value="' + result[i].MembershipType + '">' + result[i].Name + '</option>';
                }
                $('#Card_Type').html(content);
                $('#Card_Type').selectpicker('refresh');
                LoadSampleCard();
            }
        });
    }
    function LoadSampleCard() {
        if ($('#Card_Type').val() != 0) {
            $.ajax({
                type: 'POST',
                url: '@Url.Action("GetSampleMembershipCard","MembershipCard")',
                data: { 'membershipTypeId': $('#Card_Type').val() },
                success: function (result) {
                    if (result.success == true) {
                        var content = "<option value='-1'>Chọn 1 thẻ mẫu</option>";
                        for (var i = 0; i < result.memCards.length; i++) {
                            content += '<option value="' + result.memCards[i].Value + '">' + result.memCards[i].Text + '</option>';
                        }
                        $('#sampleCombobox').html(content);
                        $('#sampleCombobox').selectpicker('refresh');
                    }
                }
            });
        }
    }
    $('#Card_Type').on('change', function () {
        LoadSampleCard();
    });
    $('#sampleCombobox').on('change', function () {
        $.ajax({
            type: 'POST',
            url: '@Url.Action("GetSampleAccounts", "MembershipCard")',
            data: { 'membershipCardId': $('#sampleCombobox').val() },
            success: function (result) {
                var dataTable = $("#MemberCardDatatable").dataTable();
                dataTable.fnDestroy();
                initDataTable(result.accounts);
                listAccounts = result.accounts;
                //$(id).dataTable()._fnPageChange(0);
                //$(id).dataTable()._fnAjaxUpdate();
            }

        });
    });

    $('#btnCreateAccount').on('click', function () {
        var m = moment().add(1, 'months');
        var d = m.format("DD/MM/YYYY");
        $('#AccountFinishDateCreate').val(d);
        $('#createAccountModal').modal('show');
        onChangeAccountTypeByValue($('#create-accountType').val());
    });
    function initDataTable(data) {
        $("#MemberCardDatatable").dataTable({
            "bFilter": false,
            "bSort": false,
            "bRetrieve": true,
            "bScrollCollapse": true,
            "data": data,
            "iDisplayLength": 50,
            "aLengthMenu": [50, 150],
            "oLanguage": {
                "sSearch": "Tìm kiếm:",
                "sZeroRecords": "Không có dữ liệu phù hợp",
                "sInfo": "Hiển thị từ _START_ đến _END_ trên tổng số _TOTAL_ dòng",
                "sEmptyTable": "Không có dữ liệu",
                "sInfoFiltered": " - lọc ra từ _MAX_ dòng",
                "sLengthMenu": "Hiển thị _MENU_ dòng",
                "sProcessing": "Đang xử lý...",
                "oPaginate": {
                    "sNext": "<i class='fa fa-chevron-right'></i>",
                    "sPrevious": "<i class='fa fa-chevron-left'></i>"
                }
            },

            "aoColumnDefs": [
                {
                    "aTargets": [0, 1, 2, 3, 4, 5, 6, 7],
                    "sClass": "text-center"
                },
                {
                    "aTargets": [2],
                    "mRender": function (data, type, row) {
                        var type = row[2];
                        var str = "";

                        switch (type) {
                            case 1: str = "Tài khoản thanh thoán"; break;
                            case 2: str = "Tài khoản sản phẩm"; break;
                            case 3: str = "Tài khoản tích điểm"; break;
                        }

                        return str;
                    }
                },
                {
                    "aTargets": [6],
                    "mRender": function (data, type, row) {
                        var status = row[6];
                        if (status) {
                            return "Đã kích hoạt";
                        } else {
                            return "Chưa kích hoạt";
                        }
                    }
                },
                {
                    "aTargets": [7],
                    "mRender": function (data, type, row) {
                        //var status = row[6];
                        //var button = '<div style="display: inline-flex;"><a onclick="activeAccount(' + row[1] + ',' + false + ')" class="btn btn-sm btn-danger btnComparison"><span class="">Khóa</span></a>';
                        var button = "<div style='display: inline-flex;'><a title='Xóa' class='btn btn-danger btn-lg m-l-5' onclick='deleteMembershipCard(" + row[0] + ")'><i class='glyphicon glyphicon-trash'></i></a></div>";
                        return button;


                    },
                },
            ],
            "bAutoWidth": true,
            "order": [[1, 'asc']],
        });
    };
    function deleteMembershipCard(row) {
        if (listAccounts.length == 1) {
            listAccounts = [];
        } else {
            for (var i = row - 1; i < listAccounts.length - 1; ++i) {
                listAccounts[i + 1][0] = i + 1;
                listAccounts[i] = listAccounts[i + 1];
            }
            listAccounts.pop();
        }

        var dataTable = $("#MemberCardDatatable").dataTable();
        dataTable.fnDestroy();
        initDataTable(listAccounts);
    };
    $('#create-accountType').on('change', function () {
        onChangeAccountTypeByValue($(this).val());
    });
    function onChangeAccountTypeByValue(value) {
        $('#create-creditAccount').css("display", "none");
        $('#create-giftAccount').css("display", "none");
        $('#create-memberAccount').css("display", "none");

        switch (value) {
            case '0': $('#create-creditAccount').css("display", "block"); break;
            case '1': $('#create-giftAccount').css("display", "block"); break;
            case '2': $('#create-memberAccount').css("display", "block");
        }
    }

    $('#CreateAccountBTN').on('click', function () {

        //validate
        var typeAccount = parseInt($('#create-accountType').val(), 10) + 1;

        if ($('#create-accountName').val().trim() == '') {
            ShowMessage("Vui lòng nhập tên tài khoản", 1);
            return;
        }
        //check credit
        if (typeAccount == 1) {
            if ($('#create-credit-balance').val().trim() == '') {
                ShowMessage("Vui lòng nhập số dư tài khoản", 1);
                return;
            }

            if (parseInt($("#create-credit-balance").val()) < 0 || isNaN(parseInt($("#create-credit-balance").val()))) {

                ShowMessage("Giá trị tài khoản phải lớn hơn hoặc bằng 0", 1);
                return;
            }
        }
        if (typeAccount == 2) {
            if ($('#create-gift-balance').val().trim() == '') {
                ShowMessage("Vui lòng nhập giá trị tài khoản", 1);
                return;
            }
            if (parseInt($("#create-gift-balance").val()) < 0 || isNaN(parseInt($("#create-gift-balance").val()))) {
                ShowMessage("Giá trị tài khoản không được âm", 1);
                return;
            }
        }

        if (typeAccount == 3) {
            if ($('#create-member-balance').val().trim() == '') {
                ShowMessage("Vui lòng nhập tài khoản", 1);
                return;
            }
            if (parseInt($("#create-member-balance").val()) < 0 || isNaN(parseInt($("#create-member-balance").val()))) {
                ShowMessage("Giá trị điểm không được âm", 1);
                return;
            }
        }
        if ($('#AccountFinishDateCreate').val() != undefined && $('#AccountFinishDateCreate').val().trim() == '') {
            ShowMessage("Xin chọn ngày kết thúc", 1);
            return;
        }
        if (typeAccount == 2) {
            if ($("#create-ProductCode").val() == '-1' || $("#create-ProductCode").val() == null) {
                ShowMessage("Vui lòng chọn sản phẩm", 1);
                return;
            }
        }
        var productCode = "";
        var level = "";
        var accountName = $('#create-accountName').val();
        var finishDate = $('#AccountFinishDateCreate').val();
        if (typeAccount == 1) {
            var balance = $('#create-credit-balance').val();
            var productName = "VNĐ";
        } else if (typeAccount == 2) {
            var balance = $('#create-gift-balance').val();
            var code = $('#create-ProductCode').val();
            var productName = $('#create-ProductCode :selected').text();
            if (code != null || code == '-1') {
                productCode = code;
            }
        } else if (typeAccount == 3) {
            var balance = $('#create-member-balance').val();
            var productName = "điểm";
            level = $('#create-member-level').val();
        }

        var accountType = typeAccount;
        var accountId = 0;
        var active = 1;
        var accountTypeName = "";
        // get datetime now
        //var datetimeNow = $.now();
        //generatecode
        //var accountCode = datetimeNow + "";
        //var datetimeNow = moment();
        //var accountCode = datetimeNow.format("YYYYMMDDHHmmSSMS");

        var dataArrarytmp = [];
        var counttmp = listAccounts.length;
        dataArrarytmp.push(counttmp + 1);
        dataArrarytmp.push(accountName);
        dataArrarytmp.push(accountType);
        dataArrarytmp.push(balance);
        dataArrarytmp.push(productName);
        dataArrarytmp.push(moment().format("DD/MM/YYYY"));
        dataArrarytmp.push(1);
        dataArrarytmp.push(finishDate);
        dataArrarytmp.push(productCode);
        dataArrarytmp.push(level);
        listAccounts.push(dataArrarytmp);
        //$('#form_dangki').slideUp();
        var dataTable = $("#MemberCardDatatable").dataTable();
        dataTable.fnDestroy();
        initDataTable(listAccounts);

        clearInputModal();
        $('#createAccountModal').modal('hide');
    });


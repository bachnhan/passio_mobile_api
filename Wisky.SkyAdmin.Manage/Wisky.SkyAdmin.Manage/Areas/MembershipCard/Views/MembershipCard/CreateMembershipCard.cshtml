    function submitform() {
        if ($('#Card_Code').val() == "") {
            ShowMessage("Mã thẻ không được bỏ trống.", 1);
            return;
        }

        $.ajax({
            type: 'POST',
            url: '@Url.Action("CreateMembershipCardWithAccounts", "MembershipCard")',
            data: {
                'userName': '@HttpContext.Current.User.Identity.Name',
                'memCardCode': $('#Card_Code').val(),
                'createTime': $('#Date_Create').val(),
                'memTypeId': $('#Card_Type').val(),
                'listAccounts': JSON.stringify(listAccounts)
            },
            success: function (result) {
                if (result.success) {
                    ShowMessage("Thành công", 2);
                    window.location.href = '@Url.Action("Index","MembershipCard")';
                } else {
                    ShowMessage(result.message, 1);
                }
            }

        });
    }
</script>
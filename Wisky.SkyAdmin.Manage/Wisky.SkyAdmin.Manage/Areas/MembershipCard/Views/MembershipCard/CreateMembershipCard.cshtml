    function GetProductList() {
        $.ajax({
            type: 'GET',
            url: '@Url.Action("LoadProductList", "MembershipCard")',
            data: { 'brandId': 1 },
            success: function (result) {
                for (var i = 0; i < result.list.length + 1; i++) {
                    if (i == 0) {
                        $('#input_productCode').append('<option value="-1" checked>' + "Nothing Selected" + '</option>')
                        $('#create-ProductCode').append('<option value="-1" checked>' + "Nothing Selected" + '</option>')
                    } else {
                        $('#input_productCode').append('<option value="' + result.list[i - 1].ProductId + '" checked>' + result.list[i - 1].ProductName + '</option>')
                        $('#create-ProductCode').append('<option value="' + result.list[i - 1].ProductId + '" checked>' + result.list[i - 1].ProductName + '</option>')

                    }
                }
                $("#input_productCode").val($("#input_productCode option:first").val());
                $('#input_productCode').selectpicker('refresh');
                $("#create-ProductCode").val($("#input_productCode option:first").val());
                $('#create-ProductCode').selectpicker('refresh');

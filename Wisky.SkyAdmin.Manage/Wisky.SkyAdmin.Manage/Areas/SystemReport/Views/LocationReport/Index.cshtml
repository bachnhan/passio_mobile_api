    function cb1(start, end, label) {
        //Prepare start time and end time format
        var startTime = start.format("DD/MM/YYYY"),
            endTime = end.format("DD/MM/YYYY"),
            dateString = "(" + startTime + (startTime == endTime ? "" : " - " + endTime) + ")";

        if (label != "Tùy chọn") {
            $('#startYear1').val(label);
        } else {
            $('#startYear1').val(dateString);
        }

        $('#sTime1').val(startTime);
        $('#eTime1').val(endTime);

        //Chage date at page header
        $('[data-role=small-date]').html(dateString);

        $('#startYear1 span').html(start.format('MMM D, YYYY') + ' - ' + end.format('MMM D, YYYY'));
        loadDataOnChangeAll();
    };

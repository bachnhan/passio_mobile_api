    function GetProvinceList() {
        $.ajax({
            type: 'GET',
            url: '@Url.Action("LoadProvinceList", "LocationReport")',
            success: function (result) {
                for (var i = 0; i < result.province.length; i++) {
                    $('#provinceCombobox').append('<option value="' + result.province[i].ProvinceCode + '">' + result.province[i].ProvinceName + '</option>')
                }
                $('#provinceCombobox').val(79);
                $('#provinceCombobox').selectpicker('refresh');
                for (var i = 0; i < result.province.length; i++) {
                    $('#provinceCombobox2').append('<option value="' + result.province[i].ProvinceCode + '">' + result.province[i].ProvinceName + '</option>')
                }
                $('#provinceCombobox2').val(79);
                $('#provinceCombobox2').selectpicker('refresh');
                for (var i = 0; i < result.province.length; i++) {
                    $('#provinceCombobox3').append('<option value="' + result.province[i].ProvinceCode + '">' + result.province[i].ProvinceName + '</option>')
                }
                $('#provinceCombobox3').val(79);
                $('#provinceCombobox3').selectpicker('refresh');
                setupDaterangerpicker1();
            }

        });
    }

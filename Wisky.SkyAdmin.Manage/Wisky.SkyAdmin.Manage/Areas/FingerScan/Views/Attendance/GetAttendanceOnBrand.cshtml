        var sUpChooseRangeDiv = () => {

            //Init daterangepicker skeleton
            var dateRangeAttendace = {
                locale: {
                    format: 'DD/MM/YYYY',
                    applyLabel: 'Chấp nhận',
                    cancelLabel: 'Hủy',
                },
                startDate: moment(),
                endDate: moment(),
                ranges: {
                    'Hôm nay': [moment.utc().startOf('day'), moment.utc().endOf('day')],
                    'Hôm qua': [moment.utc().startOf('day').add(-1, 'days'), moment.utc().endOf('day').add(-1, 'days')],
                    'Tuần nay': [moment().add(0, 'week').startOf('week'), moment().add(0, 'week').endOf('week')],
                    'Tuần trước': [moment().add(-1, 'week').startOf('week'), moment().add(-1, 'week').endOf('week')],
                    'Tháng này': [moment().add(0, 'month').startOf('month'), moment().add(0, 'month').endOf('month')],
                    'Tháng sau': [moment().add(-1, 'month').startOf('month'), moment().add(-1, 'month').endOf('month')]
                },
            };
            // Init callBack function for daterangePicker
            var callBack = (start, end, label) => {
                var startTime = start.format("DD/MM/YYYY"),
                    endTime = end.format("DD/MM/YYYY"),
                    dateString = "(" + startTime + (startTime == endTime ? "" : " - " + endTime) + ")";
                if (label != "Tùy chọn") {
                    $('#chooseRangeLabel').val(label);
                } else {
                    $("#chooseRangeLabel").val(dateString);
                }
                $("#sTimeHistoryCheck").val(startTime);
                $("#eTimeHistoryCheck").val(endTime);
            };
            // start callbackFunction for the first time
            callBack(moment(), moment(), 'Hôm nay');
            // add daterangepicker to chooseDayDiv
            $('#chooseRangeDiv').daterangepicker(
                dateRangeAttendace
                , callBack);


        };


    </script>

